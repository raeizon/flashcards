from tkinter import*
from PIL import ImageTk,Image


root = Tk()
root.title('Learn to code')
root.iconbitmap()

#images
my_img = ImageTk.PhotoImage(Image.open("one.png"))
my_label= Label(image=my_img)
my_label.pack()

#exit program button
button_quit = Button(root, text="Exit Program", command=root.quit)
button_quit.pack()

root.mainloop()
